{
  "rewrites": [
    {
      "source": "/api/v1/:path*/",
      "destination": "https://codeglimpse-api.vercel.app/api/v1/:path*/"
    },
    {
      "source": "/api/v1/:path*",
      "destination": "https://codeglimpse-api.vercel.app/api/v1/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}